{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist/angular-front-end/browser",
  "framework": null,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/index.js"
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/assets/$1"
    },
    {
      "src": "/(.*\\.(js|css|ico|png|jpg|jpeg|gif|svg|woff|woff2|ttf|eot))",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ],
  "functions": {
    "api/index.js": {
      "runtime": "nodejs20.x",
      "includeFiles": "dist/angular-front-end/**",
      "memory": 1024
    }
  }
}

